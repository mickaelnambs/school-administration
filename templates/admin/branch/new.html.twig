{% extends "base.html.twig" %}
{% block title %}
	Creation d'une filiere
{% endblock %}
{% block body %}
	<div class="container-fluid mt-4">
		<div class="row">
			<div class="col-3"></div>
			<div class="col-6">
				{{ form_start(form) }}
				<div class="alert alert-light">
					{{ form_widget(form) }}
					<button class="btn btn-primary">
						<i class="fa fa-save"></i>
						Sauvegarder
					</button>
				</div>
				{{ form_end(form) }}
			</div>
			<div class="col-3"></div>
		</div>
	</div>
{% endblock %}
